<source>
  @type tail
  path /var/log/<%= @dir %>/<%= @name %>
  pos_file /var/log/td-agent/<%= @dir %>/<%= @name %>.pos
  tag <%= @dir %>-<%= @name %>
  format <%= @format %>
  time_format <%= @time_format %>
</source>

<match <%= @dir %>-<%= @name %>>
  @type s3
  format json
  s3_bucket <%= @s3_bucket %>
  s3_region <%= @s3_region %>
  path <%= @dir %>/<%= @name %>/%Y/%m/%d/
  buffer_path /var/log/td-agent/<%= @dir %>/<%= @name %>/
</match>
